{
    "name": "ellicom/coffee",
    "description": "CoffeeScript compiler for Laravel 4 (Illuminate)",
    "keywords": ["laravel", "coffee", "coffeescript", "illuminate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Talbot",
            "email": "vincent.talbot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/foundation": ">=1.0.0",
        "coffeescript/coffeescript": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": {"Ellicom\\Coffee": "src/"}
    },
    "scripts": {
        "post-package-install": "php artisan config:publish ellicom/coffee"
    },
    "minimum-stability": "dev"
}